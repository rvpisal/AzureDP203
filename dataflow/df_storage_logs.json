{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "storagelogmetrics",
						"type": "DatasetReference"
					},
					"name": "storagelogs"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "storagemetriclogsoutput",
						"type": "DatasetReference"
					},
					"name": "output"
				}
			],
			"transformations": [
				{
					"name": "selectCols"
				}
			],
			"scriptLines": [
				"source(output(",
				"          total as integer,",
				"          minimum as integer,",
				"          maximum as integer,",
				"          average as integer,",
				"          resourceId as string,",
				"          time as string,",
				"          metricName as string,",
				"          timeGrain as string,",
				"          count as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> storagelogs",
				"storagelogs select(mapColumn(",
				"          average,",
				"          time,",
				"          metricName",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectCols",
				"selectCols sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> output"
			]
		}
	}
}